{
    "name": "ws_task",
    "version": "1.0.0",
    "description": "Template for the WebSocket remote control task",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "node ./index.js",
        "start:dev": "nodemon ./index.js",
        "start:server": "NODE_OPTIONS='--experimental-specifier-resolution=node' nodemon --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm  ./dev/server.ts",
        "format": "prettier --write dev/**/*.{ts,js}",
        "lint": "eslint dev/**/*.{ts,js}",
        "build": "webpack"
    },
    "author": "Andrei Auchynnikau",
    "keywords": [],
    "license": "ISC",
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@types/node": "^22.7.9",
        "@typescript-eslint/eslint-plugin": "^8.10.0",
        "@typescript-eslint/parser": "^8.10.0",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-webpack-plugin": "^4.2.0",
        "globals": "^15.11.0",
        "nodemon": "^2.0.22",
        "prettier": "^3.3.3",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.11.0",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0"
    },
    "dependencies": {
        "buffer": "^6.0.3",
        "crypto-browserify": "^3.12.0",
        "dotenv": "^16.4.5",
        "os-browserify": "^0.3.0",
        "stream-browserify": "^3.0.0",
        "url": "^0.11.4",
        "path-browserify": "^1.0.1",
        "ws": "^8.18.0"
